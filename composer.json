{
  "name": "laravel-omnipay/omnipay-alipay",
  "type": "library",
  "description": "Alipay gateway for Omnipay payment processing library",
  "keywords": [
    "alipay",
    "gateway",
    "merchant",
    "omnipay",
    "pay",
    "payment",
    "purchase"
  ],
  "homepage": "https://github.com/laravel-omnipay/omnipay-alipay",
  "license": "MIT",
  "authors": [
    {
      "name": "Loki Else",
      "email": "laravel-omnipay@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Omnipay\\Alipay\\": "src/",
      "Omnipay\\Alipay\\Tests\\": "tests/"
    },
    "files": [
      "src/Common/helpers.php"
    ]
  },
  "require": {
    "php-http/guzzle6-adapter": "^1.1",
    "omnipay/common": "~3.0"
  },
  "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    },
  "require-dev": {
    "omnipay/tests": "~3.0"
  }
}
